input {
      file {
        path => "/home/ubuntu/elk_hello_world/test2.log"
        start_position => "beginning"
      }
}

filter {
    csv {
        separator => " "
        columns =>       [
                                                "host",
                                                "ip_public",
                                                "ip_private",
                                                "zdate"
                        ]
    }
   mutate {
        convert => {
                                                "host" => "string"
                                                "ip_public" => "string"
                                                "ip_private" => "string"
        }
   }
   date {
        match => [ "zdate", "yyyyMMdd.HHmmss" ]
   }

}

output {
    elasticsearch {
        index => "zuzu20170807.1631"
        document_type => "zdyndns"
        hosts => "http://10.92.102.111:9200"
    }
    stdout { codec => json }
}

